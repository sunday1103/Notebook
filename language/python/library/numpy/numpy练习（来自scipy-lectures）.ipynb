    "import numpy as np\n",
    "#获得1到15的一维数组\n",
    "a = np.arange(1, 16)\n",
    "#将一维数组转变为2维数组\n",
    "b = a.reshape([3,5])\n",
    "#转置\n",
    "c = b.T\n",
    "print(c)\n",
    "\n",
    "print()\n",
    "m = [2, 4]\n",
    "d = c[m]\n",
    "print(d)"
   ]
